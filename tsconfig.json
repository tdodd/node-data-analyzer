{
	"compileOnSave": true,
	"noUnusedLocals": true,
	"compilerOptions": {
		"module": "commonjs",
		"removeComments": true,
		"sourceMap": true,
		"target": "es2017",
		"watch": false,
		"noUnusedLocals": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true
	},
	"exclude": [
		".vscode",
		"node_modules",
		"release",
		"temp"
	]
}
